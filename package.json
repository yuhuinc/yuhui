    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@storybook/addon-actions": "^5.3.8",
    "@storybook/addon-info": "^5.3.9",
    "@storybook/addon-links": "^5.3.8",
